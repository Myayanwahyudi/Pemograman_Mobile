<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/utamaFragment">

    <fragment
        android:id="@+id/utamaFragment"
        android:name="myayan.wahyudi.mobile.UtamaFragment"
        android:label="fragment_utama"
        tools:layout="@layout/fragment_utama" >
        <action
            android:id="@+id/action_utamaFragment_to_keduaFragment"
            app:destination="@id/keduaFragment" />
    </fragment>
    <fragment
        android:id="@+id/keduaFragment"
        android:name="myayan.wahyudi.mobile.keduaFragment"
        android:label="fragment_kedua"
        tools:layout="@layout/fragment_kedua" >
        <action
            android:id="@+id/action_keduaFragment_to_ketigaFragment2"
            app:destination="@id/ketigaFragment2" />
    </fragment>
    <fragment
        android:id="@+id/ketigaFragment2"
        android:name="myayan.wahyudi.mobile.KetigaFragment"
        android:label="fragment_ketiga2"
        tools:layout="@layout/fragment_ketiga2" />
</navigation>